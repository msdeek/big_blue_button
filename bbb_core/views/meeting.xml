<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >


        <record id="seq_meeting_code" model="ir.sequence">
            <field name="name">Meeting ID</field>
            <field name="code">meeting.id.seq</field>
            <field name="prefix">bbb-meeting-id-</field>
            <field name="padding">50</field>
            <field name="company_id" eval="False"/>

        </record>

        <!-- Inherit Form View to Modify it -->
        <record id="bbb_meeting" model="ir.ui.view">
            <field name="name">BBB Meeting</field>
            <field name="model">slide.slide</field>
            <field name="inherit_id" ref="website_slides.view_slide_slide_form"/>
            <field name="arch" type="xml">

                <field name="slide_type" position="after">
                <!--<button name="create_meeting" type="object" string="ObjectButton"/>-->
                    <field name="is_online"/>
                    <field name="autoStartRecording"/>
                    <field name="allowStartStopRecording"/>
                    <field name="record"/>
                    <field name="bbb_server"/>
                    <field name="bbb_meeting_id"/>
                    <field name="bbb_meeting_create"/>
                    <field name="bbb_meeting_info"/>
                    <field name="join_m" widget="url" readonly="1" text="Join Meeting M"/>
                    <field name="join_a" widget="url" readonly="1" text="Join Meeting A"/>
                    <field name="start"/>
                    <field name="end"/>
                    <!-- widget="url" readonly="1" text="Join Meeting" -->
                </field>
            </field>
        </record>


    </data>
</odoo>